Boolean(bool value) struct;

True := Boolean(true)
False := Boolean(false)

not(Boolean x) Boolean {
	return Boolean(!x.value)
}
and(Boolean a, Boolean b) Boolean {
	return Boolean(a.value && b.value)
}
or(Boolean a, Boolean b) Boolean = Boolean(a.value || b.value)

convert_uint(Boolean x) bool = x.value
convert_text(Boolean x) txt {
	if x.value {
		return "true"
	}
	return "false"
}

actor main() {
	a := True
	b := False
	c := a && b
	output(c as txt)
}