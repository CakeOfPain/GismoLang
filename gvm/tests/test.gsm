gsi := GLL_LOAD("<GSI>")
gsiFetch := gsi.GLL_SYMBOL("fetch")
fetch(txt file, int status) txt = gsi.GLL_EXEC(gsiFetch, txt, file, status)

FILE_KIND    := 1
FILE_SIZE    := 2
FILE_CONTENT := 3

actor main(txt[] args) {
	if args.Size() < 2 abort@std("Not enough Arguments!")
	path := args[1]
	if (#path.fetch(FILE_KIND)) != 'f' abort@std("File does not exist or is a directory")

	content := fetch(path, FILE_CONTENT)
	content.output()
}